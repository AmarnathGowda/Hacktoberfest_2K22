#include<iostream>
using namespace std;
int main ()
{
    int A[3][3], m, n, i, j;
    
    cout << "Enter elements of matrix : ";
    for (i = 0; i < 2; i++)
        for (j = 0; j < 2; j++)
            cin >> A[i][j];
    cout << "Entered Matrix : \n ";
    for (i = 0; i < 2; i++)
    { 
        for (j = 0; j < 2; j++)
            cout << A[i][j] << " ";
        cout << "\n ";
    }
    cout << "Transpose of Matrix : \n ";
    for (i = 0; i < 2; i++)
    {
        for (j = 0; j < 2; j++)
            cout << A[j][i] << " ";
        cout << "\n ";
    }
    return 0;
}
